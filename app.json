{
  "expo": {
    "name": "studiare_frontend",
    "slug": "studiare_frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "platforms": ["ios", "android"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ios.petosbratok.studiare_frontend"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/icon.png"
      },
      "package": "com.petosbratok.studiare_frontend",
      "permissions": [
        "NOTIFICATIONS" 
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "splash": {
      "image": "./assets/icon.png",  
      "resizeMode": "contain",  
      "backgroundColor": "#ffffff"  
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      // "API_BASE_URL": "https://petosbratok.pythonanywhere.com/api",
      "API_BASE_URL": "http://192.168.0.105:8000/api",
      "eas": {
        "projectId": "80d44f41-edbe-4c09-997b-5d785c8f5204"
      }
    }
  }
}
